	<!-- Shoping Cart -->
	<form class="bg0 p-t-75 p-b-85" action="">
		<div class="container">
			<div class="row">
				<div class="col-lg-10 col-xl-7 m-lr-auto m-b-50">
					<div class="m-l-35 m-r--68 m-lr-0-xl">
						<div class="wrap-table-shopping-cart">
							<table class="table-shopping-cart">
								<tr class="table_head">
									<th class="column-1">Product</th>
									<th class="column-2"></th>
									<th class="column-3">Price</th>
									  {{!-- <th class="column-4">Quantity</th>  --}}
									<th class="column-5 "></th>
									<th class="column-6"></th>
									
								</tr>
                                 {{#each products}}
								<tr class="table_row">
									<td class="column-1">
										<div class="how-itemcart1">
											<img src="/product-images/{{this.product._id}}.jpg" alt="IMG">
										</div>
									</td>
									<td class="column-2">{{this.product.Name}}</td>
									<td class="column-3">{{this.product.price}}</td>
									
											<td>
												{{!-- <button class="btn btn-danger flex-c-m stext-90 cl2 size-40 bg8 bor13 hov-btn3 p-lr-15 trans-04 pointer m-tb-3 mt-3  "
												 style="margin-left: 36px;"  onclick="location.href='/shopping/{{this.product._id}}'">Add To Cart</button> --}}
												 <button class="btn btn-danger flex-c-m stext-90 cl2 size-40 bg8 bor13 hov-btn3 p-lr-15 trans-04 pointer m-tb-3 mt-3  "
												 style="margin-left: 16px;" onclick="shopping('{{this.product._id}}')">Add To Cart</button>
											</td>
											<td>
												<button class="btn btn-danger flex-c-m stext-90 cl2 size-40 bg8 bor13 hov-btn3 p-lr-15 trans-04 pointer m-tb-3 mt-3  "
												 style="margin-right: 11px;"  onclick="removeItem('{{this._id}}','{{this.product._id}}')">Remove</button>
											</td>
										</div>
									</td>
									
									{{!-- <td ><div class="column-5 text-success pe-5" >In Stock</div></td> --}}
								</tr>
								{{/each}}

								
							</table>
						</div>

						<div class="flex-w flex-sb-m bor15 p-t-18 p-b-15 p-lr-40 p-lr-15-sm">
