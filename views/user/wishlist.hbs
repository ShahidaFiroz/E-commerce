	<script src="js/main.js"></script>
	<script>
		function removeItem(wishId,prodId){
		$.ajax({
			url:'/removeitem',
			data:{
				wish:wishId,
				product:prodId
			},
			method:'post',
			success:(response)=>{
				if(response.removeProduct){
					location.reload()
					
					
				}
			}
		})
	}
	
	
	</script>
	<script>
	function shopping(prodId)
        {
          // console.log("+++++++++++++++++")
            $.ajax({
                url:'/shopping/'+prodId,

                method:'get',
        
            success:(response)=>{
            if(response.status){
            	location.reload()
			}
			
            }
            })
            }
            
	</script>

