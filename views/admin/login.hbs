<script>
  
    var errorEmail = document.getElementById('emailerr')
    var errorPassword = document.getElementById('passworderr')
  

    
    function validEmail() {
        const name = document.getElementById('emailId').value
        if (name == "") {
            errorEmail.innerHTML = "Enter your email address"
            return false


        }
        if (!name.match(/^\w+([\.-]?\w+)@\w+([\.-]?\w+)(\.\w{2,3})+$/)) {
            errorEmail.innerHTML = 'Enter a proper email adress'
            return false
        }

        errorEmail.innerHTML = null
        return true
    }

    function validPassowrd() {
        const name = document.getElementById('password').value
        if (name == "") {
            errorPassword.innerHTML = "Enter a password"
            return false
        }
        if (name.length < 8) {
            errorPassword.innerHTML = "Password should be  eight characters"
            return false
        }
        errorPassword.innerHTML = null
        return true
    }


    function validation() {
        if ( !validEmail() || !validPassowrd()  ) {
            return false
        }
        return true
    }
</script>